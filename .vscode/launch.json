{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Debug (Launch) Java",
            "request": "launch",
            "mainClass": "InterviewBit.src.Courses.Programming.Level4_StacksAndQueues.Problems.MultipleStack.MinStack.Java.Solution",
            "args": "",
            //"preLaunchTask": "gradle-build"
        },
        {
            "type": "java",
            "name": "Debug (Attach) Java",
            "request": "attach",
            "hostName": "localhost",
            "port": 0
        },
        {
            "name": "Debug (Launch) Python",
            "type": "python",
            "request": "launch",
            "stopOnEntry": true,
            "pythonPath": "${config:python.pythonPath}",
            "program": "${file}",
            "cwd": "${workspaceRoot}",
            "env": {},
            "envFile": "${workspaceRoot}/.env",
            "debugOptions": [
                "RedirectOutput"
            ]
        },
        {
            "name": "Attach (Remote Debug) Python",
            "type": "python",
            "request": "attach",
            "localRoot": "${workspaceRoot}",
            "remoteRoot": "${workspaceRoot}",
            "port": 3000,
            "secret": "my_secret",
            "host": "localhost"
        }
    ]
}